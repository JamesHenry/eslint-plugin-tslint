{
  "name": "eslint-plugin-tslint",
  "version": "0.0.0-development",
  "main": "lib/index.js",
  "author": "James Henry <james@henry.sc>",
  "description": "TSLint wrapper plugin for ESLint",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "tslint"
  ],
  "repository": "JamesHenry/eslint-plugin-tslint",
  "license": "MIT",
  "scripts": {
    "test": "npm run build && mocha tests --recursive",
    "build": "rm -rf lib/ && tsc",
    "semantic-release": "semantic-release pre && npm run build && npm publish && semantic-release post",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@types/node": "6.0.54",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.12.2",
    "mocha": "3.2.0",
    "semantic-release": "^6.3.2",
    "typescript": "2.0.10",
    "typescript-eslint-parser": "1.0.0"
  },
  "dependencies": {
    "tslint": "4.2.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
